const hamels = ['fastball', 'cutter', 'curve', 'change'];

function predictHamels () {
    const randomNumber = Math.floor(Math.random() * 100);
if (randomNumber < 56) {
    return hamels[0]
}   else {
    if (randomNumber > 56 && randomNumber < 69) {
    return hamels[1]
}   else {
    if (randomNumber > 69 && randomNumber < 78) {
    return hamels[2];
}   else {
    if (randomNumber > 78 && randomNumber < 100) {
    return hamels[3];
}}}}
console.log(randomNumber)
};
const hamelsNextPitch = predictHamels();
console.log(hamelsNextPitch);
